<!--pages/listTmp/listTmp.wxml-->
<template name='listTmp'>
<view class='tmpContent'>
<view class='contentTitle'>
<image src='{{avatar}}'></image>
<text>{{date}}</text>
</view>
<text class='company'>{{title}}</text>
<image src='{{detail_img}}' class='listImg'></image>
<text class='listText'>{{detail_content}}</text>
<view class='star_share'>
<image src='{{detail_love_image1}}'></image>
<text>{{love_count}}</text>
<image src='{{detail_love_image2}}'></image>
<text>66</text>
</view>
</view>
</template>
