/* pages/listTmp/listTmp.wxss */
.tmpCountent{
  display: flex;
  flex-direction: column;
  align-content: center
}
.contentTitle{
  padding: 10rpx;
}
.contentTitle image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 25rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
.contentTitle text{
  vertical-align: middle;
  font-size:26rpx;
}
.company{
  font-size: 40rpx;
  font-weight: 700;
  margin-left: 10rpx;
}
.listImg{
  width: 100%;
  height: 400rpx;
  margin: 15rpx 0;
}
.listText{
  font-size: 30rpx;
  text-indent: 30rpx;
}
.star_share image{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
  margin-left: 10rpx;
}
.star_share text{
  font-size: 24rpx;  
  vertical-align: middle;
  margin-left: 20rpx;
}